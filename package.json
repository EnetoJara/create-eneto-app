	"scripts": {
		"start": "NODE_ENV=development webpack-dev-server --config=config/webpack.dev-server.js --env.NODE_ENV=development",
		"prebuild": "rm -rf ./build",
		"build": "NODE_ENV=production GENERATE_SOURCEMAP=false node scripts/build.js",
		"prebuild:dev": "rm -rf ./dist",
		"build:dev": "NODE_ENV=production BUILDING=true webpack --config=config/webpack.dev-server.js --env.NODE_ENV=production",
		"dev:debug": "GENERATE_SOURCEMAP=true NODE_ENV=development BUILDING=false nodemon --inspect --watch server --watch config server/index.js --env.NODE_ENV=development",
		"dev": "NODE_ENV=development GENERATE_SOURCEMAP=true BUILDING=false nodemon --watch server --watch config server/index.js",
		"server": "nodemon",
		"prod": "NODE_ENV=production node ./production/server/index.js",
		"deploy": "./deploy.sh",
		"lint:c": "eslint --fix ./client/**/**/*.js",
		"lint:s": "eslint --fix ./server/**/*.js",
		"lint": "npm run lint:s && npm run lint:c"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"jest": {
		"collectCoverageFrom": [
			"client/**/*.{js,jsx,ts,tsx}",
			"!client/**/*.d.ts"
		],
		"resolver": "jest-pnp-resolver",
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"testMatch": [
			"<rootDir>/client/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/client/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
